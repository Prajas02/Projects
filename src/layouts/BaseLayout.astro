---
import Navbar from '../components/Navbar.astro'

const { title } = Astro.props; 
---

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width" />
        <meta name="generator" content={Astro.generator} />
        <title>{title}</title>
        <script src="https://kit.fontawesome.com/9d42f50124.js" crossorigin="anonymous"></script>
    </head>
    <body>
        
        <Navbar />

        <slot />

        <script is:inline type="module" src="./src/script.js"></script>
    </body>
</html>